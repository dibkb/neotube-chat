
ENVIRONMENT=development
PORT=4111
FRONTEND_URL=http://localhost:3000

# Name for the vector index/collection
INDEX_NAME=neoTubeEmbeddings

# Required: Primary Postgres connection for embeddings and app data
# Example: postgres://user:password@localhost:5432/neotube
POSTGRES_CONNECTION_STRING=

# Required: Postgres connection for agent/memory storage
# Example: postgres://user:password@localhost:5432/neotube_memory
POSTGRES_CONNECTION_STRING_MEMORY=

# Required: Postgres connection for transcripts storage
# Example: postgres://user:password@localhost:5432/neotube_transcripts
POSTGRES_CONNECTION_STRING_TRANSCRIPT=

# Required: OpenAI API key for model access
OPENAI_API_KEY=

# Required: RapidAPI credentials for YouTube transcript fetcher
RAPID_API_KEY=
RAPID_API_HOST=
